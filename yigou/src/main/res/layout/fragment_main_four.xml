<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/color_bg_ea"
    android:orientation="vertical">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp200"
            android:background="@mipmap/ic_mine_bg"
            android:paddingTop="@dimen/dp25">

            <LinearLayout
                android:id="@+id/fm_main_four_login_ll"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/dp100"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="@dimen/dp40"
                android:layout_marginBottom="@dimen/dp10"
                android:gravity="center"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/fm_main_four_head"
                    android:layout_width="@dimen/dp60"
                    android:layout_height="@dimen/dp60"
                    android:src="@mipmap/t_head" />

                <TextView
                    android:id="@+id/fm_main_four_name"
                    android:layout_width="wrap_content"
                    android:layout_height="30dp"
                    android:minWidth="@dimen/dp60"
                    android:text="立即登陆"
                    android:paddingLeft="@dimen/dp10"
                    android:paddingRight="@dimen/dp10"
                    android:gravity="center"
                    android:layout_marginTop="@dimen/dp8"
                    android:textColor="@color/colorwhite" />
            </LinearLayout>

            <ImageView
                android:id="@+id/fm_main_four_member"
                android:layout_width="@dimen/dp60"
                android:layout_height="@dimen/dp60"
                android:layout_marginLeft="@dimen/dp10"
                android:src="@mipmap/t_member_no" />

            <TextView
                android:id="@+id/fm_main_four_member_text"
                android:layout_width="80dp"
                android:layout_height="30dp"
                android:layout_below="@id/fm_main_four_member"
                android:gravity="center"
                android:text="游客"
                android:textSize="@dimen/sp12" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:background="@color/color_white"
            android:paddingLeft="@dimen/dp12"
            android:paddingRight="@dimen/dp12">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="全部订单"
                android:textColor="@color/color_name" />

            <ImageView
                android:layout_width="@dimen/dp10"
                android:layout_height="@dimen/dp10"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:src="@mipmap/ic_home_more" />

            <TextView
                android:id="@+id/fm_main_four_order_all"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:gravity="center_vertical"
                android:paddingRight="@dimen/dp15"
                android:text="查看全部订单"
                android:textColor="@color/color_content"
                android:textSize="@dimen/sp12" />


        </RelativeLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp80"
            android:layout_marginTop="@dimen/dp1"
            android:background="@color/color_white"
            android:baselineAligned="false"
            android:gravity="center"
            android:orientation="horizontal"
            android:paddingBottom="@dimen/dp10">

            <RelativeLayout
                android:id="@+id/fm_main_four_order_dzf"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1">

                <ImageView
                    android:id="@+id/img1"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:layout_marginBottom="@dimen/dp20"
                    android:paddingTop="@dimen/dp16"
                    android:paddingBottom="@dimen/dp8"
                    android:layout_centerHorizontal="true"
                    android:src="@mipmap/my_order_dzf" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp20"
                    android:layout_alignParentBottom="true"
                    android:gravity="center"
                    android:text="@string/to_be_paid"
                    android:textColor="@color/color_name" />

                <TextView
                    android:id="@+id/fm_main_four_order_dzf_num"
                    android:layout_width="@dimen/dp18"
                    android:layout_height="@dimen/dp18"
                    android:layout_marginTop="@dimen/dp6"
                    android:layout_toRightOf="@id/img1"
                    android:layout_marginLeft="-20dp"
                    android:background="@mipmap/ic_minefragment_num"
                    android:gravity="center"
                    android:maxLength="2"
                    android:text="10"
                    android:textColor="@color/textcolor_point"
                    android:textSize="@dimen/sp10"
                    android:visibility="invisible" />

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/fm_main_four_order_dfh"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1">

                <ImageView
                    android:id="@+id/img2"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:layout_marginBottom="@dimen/dp20"
                    android:paddingTop="@dimen/dp16"
                    android:paddingBottom="@dimen/dp8"
                    android:layout_centerHorizontal="true"
                    android:src="@mipmap/my_order_dfh" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp20"
                    android:layout_alignParentBottom="true"
                    android:gravity="center"
                    android:text="@string/to_send_goods"
                    android:textColor="@color/color_name" />

                <TextView
                    android:id="@+id/fm_main_four_order_dfh_num"
                    android:layout_width="@dimen/dp18"
                    android:layout_height="@dimen/dp18"
                    android:layout_marginTop="@dimen/dp6"
                    android:layout_toRightOf="@id/img2"
                    android:layout_marginLeft="-20dp"
                    android:background="@mipmap/ic_minefragment_num"
                    android:gravity="center"
                    android:maxLength="2"
                    android:text="10"
                    android:textColor="@color/textcolor_point"
                    android:textSize="@dimen/sp10"
                    android:visibility="invisible" />

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/fm_main_four_order_dsh"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1">

                <ImageView
                    android:id="@+id/img3"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:layout_marginBottom="@dimen/dp20"
                    android:paddingTop="@dimen/dp16"
                    android:paddingBottom="@dimen/dp8"
                    android:layout_centerHorizontal="true"
                    android:src="@mipmap/my_order_dsh" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp20"
                    android:layout_alignParentBottom="true"
                    android:gravity="center"
                    android:text="@string/for_goods"
                    android:textColor="@color/color_name" />

                <TextView
                    android:id="@+id/fm_main_four_order_dsh_num"
                    android:layout_width="@dimen/dp18"
                    android:layout_height="@dimen/dp18"
                    android:layout_marginTop="@dimen/dp6"
                    android:layout_toRightOf="@id/img3"
                    android:layout_marginLeft="-20dp"
                    android:background="@mipmap/ic_minefragment_num"
                    android:gravity="center"
                    android:maxLength="2"
                    android:text="10"
                    android:textColor="@color/textcolor_point"
                    android:textSize="@dimen/sp10"
                    android:visibility="invisible" />

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/fm_main_four_order_dpj"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1">

                <ImageView
                    android:id="@+id/img4"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:layout_marginBottom="@dimen/dp20"
                    android:paddingTop="@dimen/dp16"
                    android:paddingBottom="@dimen/dp8"
                    android:layout_centerHorizontal="true"
                    android:src="@mipmap/my_order_dpj" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp20"
                    android:layout_alignParentBottom="true"
                    android:gravity="center"
                    android:text="@string/to_evaluate"
                    android:textColor="@color/color_name" />

                <TextView
                    android:id="@+id/fm_main_four_order_dpj_num"
                    android:layout_width="@dimen/dp18"
                    android:layout_height="@dimen/dp18"
                    android:layout_marginTop="@dimen/dp6"
                    android:layout_toRightOf="@id/img4"
                    android:layout_marginLeft="-20dp"
                    android:background="@mipmap/ic_minefragment_num"
                    android:gravity="center"
                    android:maxLength="2"
                    android:text="10"
                    android:textColor="@color/textcolor_point"
                    android:textSize="@dimen/sp10"
                    android:visibility="invisible" />

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/fm_main_four_order_tk"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1">

                <ImageView
                    android:id="@+id/img5"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:layout_marginBottom="@dimen/dp20"
                    android:paddingTop="@dimen/dp16"
                    android:paddingBottom="@dimen/dp6"
                    android:layout_centerHorizontal="true"
                    android:src="@mipmap/my_order_tk" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp20"
                    android:layout_alignParentBottom="true"
                    android:gravity="center"
                    android:text="@string/refund"
                    android:textColor="@color/color_name" />

                <TextView
                    android:id="@+id/fm_main_four_order_tk_num"
                    android:layout_width="@dimen/dp18"
                    android:layout_height="@dimen/dp18"
                    android:layout_marginTop="@dimen/dp6"
                    android:layout_toRightOf="@id/img5"
                    android:layout_marginLeft="-20dp"
                    android:background="@mipmap/ic_minefragment_num"
                    android:gravity="center"
                    android:maxLength="2"
                    android:text="10"
                    android:textColor="@color/textcolor_point"
                    android:textSize="@dimen/sp10"
                    android:visibility="invisible" />

            </RelativeLayout>
        </LinearLayout>
        <RelativeLayout
            android:id="@+id/fm_main_four_signin"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp90"
            android:layout_marginTop="@dimen/dp8"
            android:background="@mipmap/signin_rel">

        </RelativeLayout>

        <!--收货地址-->
        <RelativeLayout
            android:id="@+id/fm_main_four_rl_address"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp45"
            android:layout_marginTop="@dimen/dp8"
            android:background="@color/color_white">

            <ImageView
                android:layout_width="@dimen/dp45"
                android:layout_height="match_parent"
                android:padding="@dimen/dp12"
                android:src="@mipmap/my_item_address" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dp45"
                android:text="收货地址"
                android:textColor="@color/color_name"
                android:textSize="@dimen/sp14" />

            <ImageView
                android:layout_width="@dimen/dp45"
                android:layout_height="match_parent"
                android:layout_alignParentRight="true"
                android:src="@mipmap/t_right"
                android:padding="@dimen/dp15"
                />
        </RelativeLayout>
        <!--会员中心-->
        <RelativeLayout
            android:id="@+id/fm_main_four_rl_vip"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp45"
            android:layout_marginTop="@dimen/dp1"
            android:background="@color/color_white">

            <ImageView
                android:layout_width="@dimen/dp45"
                android:layout_height="match_parent"
                android:padding="@dimen/dp12"
                android:src="@mipmap/my_item_vip" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dp45"
                android:text="会员中心"
                android:textColor="@color/color_name"
                android:textSize="@dimen/sp14" />

            <ImageView
                android:layout_width="@dimen/dp45"
                android:layout_height="match_parent"
                android:layout_alignParentRight="true"
                android:src="@mipmap/t_right"
                android:padding="@dimen/dp15"
                />
        </RelativeLayout>
        <!--邀请有礼-->
        <RelativeLayout
            android:id="@+id/fm_main_four_rl_yaoqing"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp45"
            android:layout_marginTop="@dimen/dp1"
            android:background="@color/color_white">

            <ImageView
                android:layout_width="@dimen/dp45"
                android:layout_height="match_parent"
                android:padding="@dimen/dp12"
                android:src="@mipmap/my_item_yqyj" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dp45"
                android:text="邀请有礼"
                android:textColor="@color/color_name"
                android:textSize="@dimen/sp14" />

            <ImageView
                android:layout_width="@dimen/dp45"
                android:layout_height="match_parent"
                android:layout_alignParentRight="true"
                android:src="@mipmap/t_right"
                android:padding="@dimen/dp15"
                />
        </RelativeLayout>
        <!--我的粉丝-->
        <RelativeLayout
            android:id="@+id/fm_main_four_rl_fens"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp45"
            android:layout_marginTop="@dimen/dp1"
            android:background="@color/color_white">

            <ImageView
                android:layout_width="@dimen/dp45"
                android:layout_height="match_parent"
                android:padding="@dimen/dp12"
                android:src="@mipmap/my_item_wdfs" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dp45"
                android:text="我的粉丝"
                android:textColor="@color/color_name"
                android:textSize="@dimen/sp14" />

            <ImageView
                android:layout_width="@dimen/dp45"
                android:layout_height="match_parent"
                android:layout_alignParentRight="true"
                android:src="@mipmap/t_right"
                android:padding="@dimen/dp15"
                />

        </RelativeLayout>
        <!--我的收益-->
        <RelativeLayout
            android:id="@+id/fm_main_four_rl_shouyi"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp45"
            android:layout_marginTop="@dimen/dp1"
            android:background="@color/color_white">

            <ImageView
                android:layout_width="@dimen/dp45"
                android:layout_height="match_parent"
                android:padding="@dimen/dp12"
                android:src="@mipmap/my_item_wdsy" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dp45"
                android:text="我的收益"
                android:textColor="@color/color_name"
                android:textSize="@dimen/sp14" />

            <ImageView
                android:layout_width="@dimen/dp45"
                android:layout_height="match_parent"
                android:layout_alignParentRight="true"
                android:src="@mipmap/t_right"
                android:padding="@dimen/dp15"
                />

        </RelativeLayout>
        <!--收益排名-->
        <RelativeLayout
            android:id="@+id/fm_main_four_rl_shouyi_sort"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp45"
            android:layout_marginTop="@dimen/dp1"
            android:background="@color/color_white">

            <ImageView
                android:layout_width="@dimen/dp45"
                android:layout_height="match_parent"
                android:padding="@dimen/dp12"
                android:src="@mipmap/my_item_sypm" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dp45"
                android:text="收益排名"
                android:textColor="@color/color_name"
                android:textSize="@dimen/sp14" />

            <ImageView
                android:layout_width="@dimen/dp45"
                android:layout_height="match_parent"
                android:layout_alignParentRight="true"
                android:src="@mipmap/t_right"
                android:padding="@dimen/dp15"
                />

        </RelativeLayout>
        <!--我的优惠券-->
        <RelativeLayout
            android:id="@+id/fm_main_four_rl_yhj"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp45"
            android:layout_marginTop="@dimen/dp1"
            android:background="@color/color_white">

            <ImageView
                android:layout_width="@dimen/dp45"
                android:layout_height="match_parent"
                android:padding="@dimen/dp12"
                android:src="@mipmap/my_item_yhj" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dp45"
                android:text="我的优惠券"
                android:textColor="@color/color_name"
                android:textSize="@dimen/sp14" />

            <ImageView
                android:layout_width="@dimen/dp45"
                android:layout_height="match_parent"
                android:layout_alignParentRight="true"
                android:src="@mipmap/t_right"
                android:padding="@dimen/dp15"
                />
        </RelativeLayout>
        <!--设置-->
        <RelativeLayout
            android:id="@+id/fm_main_four_rl_setting"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp45"
            android:layout_marginTop="@dimen/dp1"
            android:background="@color/color_white">

            <ImageView
                android:layout_width="@dimen/dp45"
                android:layout_height="match_parent"
                android:padding="@dimen/dp12"
                android:src="@mipmap/my_item_setting" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dp45"
                android:text="设置"
                android:textColor="@color/color_name"
                android:textSize="@dimen/sp14" />

            <ImageView
                android:layout_width="@dimen/dp45"
                android:layout_height="match_parent"
                android:layout_alignParentRight="true"
                android:src="@mipmap/t_right"
                android:padding="@dimen/dp15"
                />
        </RelativeLayout>
    </LinearLayout>
</ScrollView>
