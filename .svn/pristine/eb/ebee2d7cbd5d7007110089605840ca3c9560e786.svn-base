package com.t.httplib.yigou.bean.req;

/**
 * 请求商品列表数据的参数Bean
 */
public class ReqGoodsListInfo extends ReqPageInfo {

    //品牌id
    private int brandId;
    //综合排序
    private int whole;
    //销量
    private int sell;
    //价格
    private int price;
    //店铺id
    private int shopId;

    // 商品分类：0：全部；其它：传对应分类编号
    private int relatedCatids;

    public int getBrandId() {
        return brandId;
    }

    public void setBrandId(int brandId) {
        this.brandId = brandId;
    }

    public int getWhole() {
        return whole;
    }

    public void setWhole(int whole) {
        this.whole = whole;
    }

    public int getSell() {
        return sell;
    }

    public void setSell(int sell) {
        this.sell = sell;
    }

    public int getPrice() {
        return price;
    }

    public void setPrice(int price) {
        this.price = price;
    }

    public int getShopId() {
        return shopId;
    }

    public void setShopId(int shopId) {
        this.shopId = shopId;
    }

    public int getRelatedCatids() {
        return relatedCatids;
    }

    public void setRelatedCatids(int relatedCatids) {
        this.relatedCatids = relatedCatids;
    }

    @Override
    public String toString() {
        return "brandId" + brandId +
                "page" + page +
                "pageSize" + pageSize +
                "price" + price +
                "relatedCatids" + relatedCatids +
                "sell" + sell +
                "shopId" + shopId +
                "timestamp" + timestamp +
                "whole" + whole;
    }
}
